<%= form_with(model: Post.new, local: true) do |form| %>
  <div class="post_name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="post_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
